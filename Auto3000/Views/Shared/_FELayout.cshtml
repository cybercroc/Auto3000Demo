    <script type="text/javascript">
        $(function () {

        });

        $(document).ready(function () {


            $("#sliderRange").slider({
                range: true,
                min: 0,
                max: 500,
                values: [50, 500],
                slide: function (event, ui) {
                    $("#amount").val("$" + ui.values[0] + " - $" + ui.values[1]);
                }
            });



            $('#slideshow').cycle({
                prev: '#prev',
                next: '#next',
                fx: 'scrollHorz',
                easing: 'easeOutBack',
                speed: 1300,
                timeout: 8000
            });
        });
    </script>

    <link href="includes/styles/jquery-ui-1.10.3.custom.min.css" rel="stylesheet" type="text/css" />
